#pragma once

// The numeric version format is AAAAABBBBBCCCCCDDDE where:
//
// AAAAA - major version number
// BBBBB - minor version number
// CCCCC - bugfix version number
// DDD   - alpha / beta (DDD + 500) version number
// E     - final (0) / snapshot (1)
//
// When DDDE is not 0, 1 is subtracted from AAAAABBBBBCCCCC. For example:
//
// Version      AAAAABBBBBCCCCCDDDE
//
// 0.1.0        0000000001000000000
// 0.1.2        0000000001000020000
// 1.2.3        0000100002000030000
// 2.2.0-a.1    0000200001999990010
// 3.0.0-b.2    0000299999999995020
// 2.2.0-a.1.z  0000200001999990011
//
#define LIBIMPLOT_VERSION       $libimplot.version.project_number$ULL
#define LIBIMPLOT_VERSION_STR   "$libimplot.version.project$"
#define LIBIMPLOT_VERSION_ID    "$libimplot.version.project_id$"
#define LIBIMPLOT_VERSION_FULL  "$libimplot.version$"

#define LIBIMPLOT_VERSION_MAJOR $libimplot.version.major$
#define LIBIMPLOT_VERSION_MINOR $libimplot.version.minor$
#define LIBIMPLOT_VERSION_PATCH $libimplot.version.patch$

#define LIBIMPLOT_PRE_RELEASE   $libimplot.version.pre_release$

#define LIBIMPLOT_SNAPSHOT_SN   $libimplot.version.snapshot_sn$ULL
#define LIBIMPLOT_SNAPSHOT_ID   "$libimplot.version.snapshot_id$"
